    "gh-pages": "^6.1.1",
    "json-server": "^1.0.0-beta.1",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.4",
    "vite": "^5.3.4",
    "vite-plugin-mock": "^3.0.2"
