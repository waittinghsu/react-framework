    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.25.1",
    "redux": "^5.0.1",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^3.1.0",
    "sass": "^1.77.8",
    "uuid": "^10.0.0"
